{
  "name": "mvvm",
  "version-string": "0.0.1",
  "license": "MIT",
  "dependencies": [
    "opengl",
    "matchit",
    "boost-format",
    "boost-log",
    {
      "name": "glfw3",
      "platform": "!emscripten"
    },
    {
      "name": "imgui",
      "default-features": false,
      "features": [
        "docking-experimental",
        "wchar32",
        "opengl3-binding",
        "glfw-binding"
      ],
      "platform": "!emscripten"
    },
    {
      "name": "imgui",
      "default-features": false,
      "features": [
        "docking-experimental",
        "wchar32",
        "opengl3-binding",
        "glfw-binding"
      ],
      "platform": "emscripten"
    },
    {
      "name": "gtest"
    },
    {
      "name": "freetype",
      "default-features": false,
      "features": [
        "png",
        "zlib",
        "bzip2"
      ]
    }
  ]
}